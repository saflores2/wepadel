  <h1>Create your tournament</h1>
  <div class="container">
    <%= simple_form_for(@tournament) do |f| %>
      <%= f.input :name %>
      <%= f.input :ubication_address %>
      <%= f.input :ubication_name %>
      <%= f.input :price %>
      <%= f.input :start_date %>
      <%= f.input :end_date %>
      <%= f.input :duration %>
      <%= f.input :category, collection: ["Masculino 1ra", "Masculino 2da", "Masculino 3ra", "Masculino 4ta", "Masculino 5ta", "Masculino 6ta", "Femenino A", "Femenino B", "Femenino C", "Femenino D", "Mixto A", "Mixto B", "Mixto C", "Mixto D", "Otra"] %>
      <%= f.input :min_matches %>
      <%= f.input :max_matches %>
      <%= f.input :places %>
      <%= f.input :match_duration %>
      <%= f.input :awards %>
      <%= f.input :other %>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit, "Save" %>
    <% end %>

    <%= link_to "Back to tournaments", tournaments_path %>
